properties([parameters([choice(choices: 'main\ndev', description: 'Select Branch to build', name: 'branch')])])

node{
    stage('Scm Checkout'){
        echo "Pulling changes from the branch ${params.branch}"
        git url: 'https://github.com/Cloudbci/simple-java-app.git', branch: "${params.branch}"
    }
    
}
